<Window x:Class="Ejer5MVVM.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejer5MVVM.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Height="23" Margin="0,0,0,0" Text="{Binding TbDerecho, UpdateSourceTrigger=LostFocus}" Width="120"/>
        <TextBox Height="23" Margin="0,50,0,0" Text="{Binding TbReves, UpdateSourceTrigger=PropertyChanged}" Width="120"/>
    </Grid>
</Window>
